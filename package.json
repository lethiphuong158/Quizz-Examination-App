{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,yml,yaml,md}": "prettier --write"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext=js,jsx,ts,tsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext=js,jsx,ts,tsx --fix --ignore-path .gitignore",
    "format": "prettier --write **/*.{js,jsx,ts,tsx,css,md,json} --ignore-path .gitignore",
    "format:check": "prettier --check **/*.{js,jsx,ts,tsx,css,md,json} --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-common-types": "^0.2.35",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@types/bootstrap": "^5.0.17",
    "axios": "^0.21.1",
    "bootstrap": "^5.0.2",
    "classnames": "^2.3.1",
    "js-cookie": "^3.0.0",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest",
    "sass": "^1.35.2",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/js-cookie": "^2.2.7",
    "@types/react": "17.0.14",
    "@types/styled-components": "^5.1.11",
    "eslint": "7.26.0",
    "eslint-config-kinal": "1.0.3",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "prettier": "2.3.0",
    "typescript": "^4.3.5"
  }
}
